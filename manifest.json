{
    "name": "Adblocker Extreme",
    "description": "Sure it's an adblocker, why not! Just download it please...",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs",
        "fontSettings",
        "tts"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "res/scream.mp3",
                "res/creepyblended.png",
                "res/fireball.png",
                "res/dwagon.png",
                "res/d20.gif",
                "res/devito1.png",
                "res/devito2.png",
                "res/devito3.png",
                "res/devito4.png",
                "res/devito5.png",
                "res/devito6.png",
                "res/devito7.png"

            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "action": {
        "default_popup": "html/main_page.html"
    },
    "host_permissions": [
        "*://*/*"
    ]
}